{
    "name": "jackalopelabs/jackalopes-server",
    "description": "Multiplayer server for Jackalopes FPS game",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mason Lawlor",
            "email": "mason@jackalope.io"
        }
    ],
    "require": {
        "php": ">=8.1",
        "composer/installers": "^2.0",
        "react/socket": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Jackalopes\\Server\\": "src/"
        },
        "classmap": [
            "includes/",
            "admin/"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "minimum-stability": "stable"
}